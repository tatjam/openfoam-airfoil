FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear 1;
}

divSchemes
{
    default         Gauss linear;
	div(phi,U) 	bounded Gauss linearUpwind default;
	div(phi,k) 	bounded Gauss upwind;
	div(phi,omega) bounded Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p               ;
    Phi             ;
}
wallDist
{
    method          meshWave;
}


